{
  "expo": {
    "name": "PG Management",
    "slug": "pgmanagement",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B82F6"
    },
    "assetBundlePatterns": [
      "assets/fonts/**",
      "assets/app-icon.png",
      "assets/splash-logo.png"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pgmanagement.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification",
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "package": "com.pgmanagement.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#3B82F6"
      },
      "permissions": [
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS",
        "INTERNET",
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS",
        "INTERNET"
      ],
      "googleServicesFile": "./android/app/google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "pgapp",
              "host": "payment-result"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "pgapp",
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#3B82F6",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0f6ecb0b-7511-427b-be33-74a4bd0207fe"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/0f6ecb0b-7511-427b-be33-74a4bd0207fe"
    }
  }
}
